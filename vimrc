filetype plugin on
filetype plugin indent on

colorscheme retrobox
syntax on
set wildmenu
set showcmd
set hlsearch
set incsearch
set cindent
set ts=4
set sw=4
set tags +=./tags,tags;
set nocompatible
set backspace=3
set nu
set showcmd

nmap <CR> o<Esc>
nmap <C-h> :nohl<CR>
nmap T :tabnew
nmap H gT
nmap L gt
